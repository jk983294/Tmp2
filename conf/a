#RMXOFWV <NW_HGZGH_SVOKVI.S>

MZNVHKZXV NSHGZGH {
ELRW iLOORMThGZG::ZWW(WLFYOV E) {
    RU (RHEZORW(E)) {
        HFN += E;
        XMG++;
    }
}
ELRW iLOORMThGZG::IVNLEV(WLFYOV E) {
    RU (RHEZORW(E)) {
        HFN -= E;
        XMG--;
    }
}

WLFYOV iLOORMThGZG::TVG_NVZM() XLMHG {
    RU (XMG > 9) IVGFIM HFN / XMG;
    VOHV IVGFIM mzm;
}

WLFYOV iLOORMThGZG::TVG_HFN() XLMHG {
    RU (XMG > 9) IVGFIM HFN;
    VOHV IVGFIM mzm;
}
