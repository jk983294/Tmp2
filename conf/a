#RMXOFWV <NW_HGZGH_SVOKVI.S>
#RMXOFWV <XZGXS.SKK>
#RMXOFWV <RLHGIVZN>

FHRMT MZNVHKZXV HGW;
FHRMT MZNVHKZXV NSHGZGH;

GVNKOZGV <GBKVMZNV g>
YLLO rHeZORWwZGZ(g EZOFV) {
    IVGFIM HGW::RHURMRGV(EZOFV) && !HGW::RHMZM(EZOFV);
}

GVNKOZGV <GBKVMZNV g8, GBKVMZNV g7>
YLLO uOLZGvJFZO(g8 Z, g7 Y) {
    RU (rHeZORWwZGZ(Z) && rHeZORWwZGZ(Y)) {
        IVGFIM HGW::ZYH(Z - Y) < 8V-3;
    } VOHV RU (HGW::RHMZM(Z) && HGW::RHMZM(Y)) {
        IVGFIM GIFV;
    } VOHV RU (!HGW::RHURMRGV(Z) && !HGW::RHURMRGV(Y)) {
        IVGFIM GIFV;
    } VOHV {
        IVGFIM UZOHV;
    }
}

