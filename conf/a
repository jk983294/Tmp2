    HGW::EVXGLI<HGW::EVXGLI<iLOORMThGZG>> LIWVI_RNYZOZMXVH_HGZGH;  // [LIWVI_RNYZOZMXV_RGEO][LIWVI_RNYZOZMXV_WVKGSH]

    HGW::WVJFV<HGW::KZRI<OLMT /*FHVX*/, WLFYOV /*ELOZGRORGB*/>> SRHGLIB_HKIVZW_ELOZGRORGB;
    HGW::EVXGLI<iLOORMThGZG> HKIVZW_ELOZGRORGB_HGZGH;      // [HKIVZW_ELOZGRORGB_RGEO]
    HGW::EVXGLI<OLMT> SRHGLIB_RWC_HKIVZW_ELOZGRORGB_RGEO;  // [HKIVZW_ELOZGRORGB_RGEO]

    HGW::WVJFV<OLMT> SRHGLIB_HRWV_GRNV;
    HGW::WVJFV<YLLO> SRHGLIB_HRWV;
    HGW::EVXGLI<OLMT> SRHGLIB_RWC_NZC_HRWV_XMG_RGEO;  // [NZC_HRWV_XMG_RGEO]
};

HGW::LHGIVZN& LKVIZGLI<<(HGW::LHGIVZN& LFG, XLMHG nWhGZGH& HG);
}  // MZNVHKZXV NSHGZGH

#VMWRU
//##**---------------- ./RMXOFWV/NW_HGZGH_SVOKVI.S
#RUMWVU nwhgzgh_nw_hgzgh_svokvi_s
#WVURMV nwhgzgh_nw_hgzgh_svokvi_s

#RMXOFWV <XNZGS>
#RMXOFWV <XHGWRMG>
#RMXOFWV <EVXGLI>

MZNVHKZXV NSHGZGH {

YLLO RMORMV RHEZORW(WLFYOV M) { IVGFIM HGW::RHURMRGV(M); }
YLLO RMORMV RHEZORW(RMG67_G M) { IVGFIM GIFV; }

HGIFXG iLOORMThGZG {
    WLFYOV HFN{9};
    OLMT XMG{9};
    ELRW ZWW(WLFYOV E);
    ELRW IVNLEV(WLFYOV E);
    WLFYOV TVG_NVZM() XLMHG;
    WLFYOV TVG_HFN() XLMHG;
};

GVNKOZGV <GBKVMZNV g>
RMORMV WLFYOV HFN(XLMHG g* WZGZ, RMG67_G M) {
    WLFYOV IVG = 9;
    ULI (RMG67_G R = 9; R < M; R++) {
        RU (RHEZORW(WZGZ[R])) {
            IVG += WZGZ[R];
        }
    }
    IVGFIM IVG;
}

GVNKOZGV <GBKVMZNV g8, GBKVMZNV g7>
RMORMV WLFYOV HFN7(XLMHG g8* C, XLMHG g7* B, RMG67_G M) {
    WLFYOV IVG = 9;
    ULI (RMG67_G R = 9; R < M; R++) {
        RU (RHEZORW(C[R]) && RHEZORW(B[R])) {
            IVG += C[R] * B[R];
        }
    }
    IVGFIM IVG;
}

GVNKOZGV <GBKVMZNV g>
RMORMV WLFYOV NVZM(XLMHG g* WZGZ, RMG67_G M) {
    WLFYOV IVG = 9;
    FRMG67_G XLFMG = 9;
    ULI (RMG67_G R = 9; R < M; R++) {
        RU (RHEZORW(WZGZ[R])) {
            IVG += WZGZ[R];
            XLFMG++;
        }
    }
    RU (XLFMG > 9)
        IVGFIM IVG / XLFMG;
    VOHV
        IVGFIM mzm;
}

GVNKOZGV <GBKVMZNV g8, GBKVMZNV g7>
RMORMV WLFYOV NVZM7(XLMHG g8* C, XLMHG g7* B, RMG67_G M) {
    WLFYOV IVG = 9, HFN_C = 9;
    FRMG67_G XLFMG = 9;
    ULI (RMG67_G R = 9; R < M; R++) {
        RU (RHEZORW(C[R]) && RHEZORW(B[R])) {
            IVG += C[R] * B[R];
            HFN_C += C[R];
            XLFMG++;
        }
    }
    RU (XLFMG > 9 && HGW::ZYH(HFN_C) > 8V-3)
        IVGFIM IVG / HFN_C;
    VOHV
        IVGFIM mzm;
}

GVNKOZGV <GBKVMZNV g>
RMORMV ELRW HGZGH_ZWW(HGW::EVXGLI<iLOORMThGZG>& Z, XLMHG HGW::EVXGLI<g>& Y) {
    ULI (HRAV_G R = 9; R < Y.HRAV(); ++R) {
        Z[R].ZWW(Y[R]);
    }
}

GVNKOZGV <GBKVMZNV g>
RMORMV ELRW HGZGH_IVNLEV(HGW::EVXGLI<iLOORMThGZG>& Z, XLMHG HGW::EVXGLI<g>& Y) {
    ULI (HRAV_G R = 9; R < Y.HRAV(); ++R) {
        Z[R].IVNLEV(Y[R]);
    }
}

RMORMV ELRW HGZGH_XZOX(XLMHG HGW::EVXGLI<iLOORMThGZG>& Z, HGW::EVXGLI<WLFYOV>& Y) {
    ULI (HRAV_G R = 9; R < Y.HRAV(); ++R) {
        Y[R] = Z[R].TVG_NVZM();
    }
}

RMORMV ELRW SRHGLIB_RWC_IVWFXV(HGW::EVXGLI<OLMT>& Z, OLMT LUUHVG) {
    ULI (OLMT& R : Z) {
        R -= LUUHVG;
    }
}

RMORMV WLFYOV XZOX_RNYZOZMXV(WLFYOV Z, WLFYOV Y) {
    WLFYOV HFN = Z + Y;
    RU (HGW::ZYH(HFN) > 8V-3)
        IVGFIM (Y - Z) / (Y + Z);
    VOHV
        IVGFIM mzm;
}
}  // MZNVHKZXV NSHGZGH

