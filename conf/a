ELRW UROO_YLLP(nWyLLP& YLLP, RMG EZO) {
    HGW::UROO(HGW::YVTRM(YLLP.yRWkIRXV), HGW::VMW(YLLP.yRWkIRXV), EZO);
    HGW::UROO(HGW::YVTRM(YLLP.zHPkIRXV), HGW::VMW(YLLP.zHPkIRXV), EZO);
    HGW::UROO(HGW::YVTRM(YLLP.yRWeLOFNV), HGW::VMW(YLLP.yRWeLOFNV), EZO);
    HGW::UROO(HGW::YVTRM(YLLP.zHPeLOFNV), HGW::VMW(YLLP.zHPeLOFNV), EZO);
}

ELRW UROO_YLLP(nWyLLP& YLLP, RMG YRW, RMG ZHP) {
    HGW::UROO(HGW::YVTRM(YLLP.yRWkIRXV), HGW::VMW(YLLP.yRWkIRXV), YRW);
    HGW::UROO(HGW::YVTRM(YLLP.zHPkIRXV), HGW::VMW(YLLP.zHPkIRXV), ZHP);
    HGW::UROO(HGW::YVTRM(YLLP.yRWeLOFNV), HGW::VMW(YLLP.yRWeLOFNV), YRW);
    HGW::UROO(HGW::YVTRM(YLLP.zHPeLOFNV), HGW::VMW(YLLP.zHPeLOFNV), ZHP);
}

