#RMXOFWV <NW_HGZGH.S>
#RMXOFWV <ZOTLIRGSN>
#RMXOFWV <XNZGS>

MZNVHKZXV NSHGZGH {
nWhGZGH::nWhGZGH(XLMHG nWhGZGxLMURT& XLMURT) : N_KxLMURT{&XLMURT} {
    RMHGZMG_YRW_GLGZO_ELOFNVH.IVHRAV(XLMURT.ORJFRWRGB_WVKGSH.HRAV(), 9);
    RMHGZMG_ZHP_GLGZO_ELOFNVH.IVHRAV(XLMURT.ORJFRWRGB_WVKGSH.HRAV(), 9);
    RMHGZMG_YRW_ZET_KIRXVH.IVHRAV(XLMURT.ORJFRWRGB_WVKGSH.HRAV(), 9);
    RMHGZMG_ZHP_ZET_KIRXVH.IVHRAV(XLMURT.ORJFRWRGB_WVKGSH.HRAV(), 9);
    NVZM_YRW_GLGZO_ELOFNVH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(),
                                  HGW::EVXGLI<WLFYOV>(XLMURT.ORJFRWRGB_WVKGSH.HRAV(), mzm));
    NVZM_ZHP_GLGZO_ELOFNVH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(),
                                  HGW::EVXGLI<WLFYOV>(XLMURT.ORJFRWRGB_WVKGSH.HRAV(), mzm));
    NVZM_YRW_ZET_KIRXVH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(), HGW::EVXGLI<WLFYOV>(XLMURT.ORJFRWRGB_WVKGSH.HRAV(), mzm));
    NVZM_ZHP_ZET_KIRXVH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(), HGW::EVXGLI<WLFYOV>(XLMURT.ORJFRWRGB_WVKGSH.HRAV(), mzm));
    NVZM_HKIVZWH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(), mzm);
    SRHGLIB_RWC_ORJFRWRGB_RGEO.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(), 9);
    YRW_GLGZO_ELOFNVH_HGZGH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(),
                                   HGW::EVXGLI<iLOORMThGZG>(XLMURT.ORJFRWRGB_WVKGSH.HRAV()));
    ZHP_GLGZO_ELOFNVH_HGZGH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(),
                                   HGW::EVXGLI<iLOORMThGZG>(XLMURT.ORJFRWRGB_WVKGSH.HRAV()));
    YRW_ZET_KIRXVH_HGZGH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(), HGW::EVXGLI<iLOORMThGZG>(XLMURT.ORJFRWRGB_WVKGSH.HRAV()));
    ZHP_ZET_KIRXVH_HGZGH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV(), HGW::EVXGLI<iLOORMThGZG>(XLMURT.ORJFRWRGB_WVKGSH.HRAV()));
    HKIVZW_HGZGH.IVHRAV(XLMURT.ORJFRWRGB_RGEO.HRAV());

    KIRXV_IVZORAVW_ELOZGRORGRVH.IVHRAV(XLMURT.KIRXV_ELOZGRORGB_XMG.HRAV(), mzm);
    RU (!XLMURT.KIRXV_ELOZGRORGB_XMG.VNKGB()) {
        GIZWV_KIRXVH = MVD gwlMORMV::iRMTzIIZB<WLFYOV>(XLMURT.KIRXV_ELOZGRORGB_XMG.YZXP() + 8);
    }

    YFB_GLGZO_ELOFNVH.IVHRAV(XLMURT.GIZWV_ELOFNV_RGEO.HRAV(), 9);
    HVOO_GLGZO_ELOFNVH.IVHRAV(XLMURT.GIZWV_ELOFNV_RGEO.HRAV(), 9);
    GIZWV_RNYZOZMXVH.IVHRAV(XLMURT.GIZWV_ELOFNV_RGEO.HRAV(), mzm);
    SRHGLIB_RWC_YFB_GIZWV_ELOFNV_RGEO.IVHRAV(XLMURT.GIZWV_ELOFNV_RGEO.HRAV(), 9);
    SRHGLIB_RWC_HVOO_GIZWV_ELOFNV_RGEO.IVHRAV(XLMURT.GIZWV_ELOFNV_RGEO.HRAV(), 9);
    YFB_GIZWV_ELOFNV_HGZGH.IVHRAV(XLMURT.GIZWV_ELOFNV_RGEO.HRAV());
    HVOO_GIZWV_ELOFNV_HGZGH.IVHRAV(XLMURT.GIZWV_ELOFNV_RGEO.HRAV());

    RMHGZMG_LIWVI_RNYZOZMXVH.IVHRAV(XLMURT.LIWVI_RNYZOZMXV_WVKGSH.HRAV(), 9);
    NVZM_LIWVI_RNYZOZMXVH.IVHRAV(XLMURT.LIWVI_RNYZOZMXV_RGEO.HRAV(),
                                 HGW::EVXGLI<WLFYOV>(XLMURT.LIWVI_RNYZOZMXV_WVKGSH.HRAV(), mzm));
    SRHGLIB_RWC_LIWVI_RNYZOZMXV_RGEO.IVHRAV(XLMURT.LIWVI_RNYZOZMXV_RGEO.HRAV(), 9);
    LIWVI_RNYZOZMXVH_HGZGH.IVHRAV(XLMURT.LIWVI_RNYZOZMXV_RGEO.HRAV(),
                                  HGW::EVXGLI<iLOORMThGZG>(XLMURT.LIWVI_RNYZOZMXV_WVKGSH.HRAV()));

    HKIVZW_ELOZGRORGRVH.IVHRAV(XLMURT.HKIVZW_ELOZGRORGB_RGEO.HRAV(), mzm);
    HKIVZW_ELOZGRORGB_HGZGH.IVHRAV(XLMURT.HKIVZW_ELOZGRORGB_RGEO.HRAV());
    SRHGLIB_RWC_HKIVZW_ELOZGRORGB_RGEO.IVHRAV(XLMURT.HKIVZW_ELOZGRORGB_RGEO.HRAV(), 9);

    YRW_HRWV_XMGH.IVHRAV(XLMURT.NZC_HRWV_XMG_RGEO.HRAV(), mzm);
    ZHP_HRWV_XMGH.IVHRAV(XLMURT.NZC_HRWV_XMG_RGEO.HRAV(), mzm);
    SRHGLIB_RWC_NZC_HRWV_XMG_RGEO.IVHRAV(XLMURT.NZC_HRWV_XMG_RGEO.HRAV(), 9);
}

ELRW nWhGZGH::lMyLLP(OLMT FHVX, XLMHG nWyLLP& YLLP) {
    ULI (HRAV_G R = 9; R < N_KxLMURT->ORJFRWRGB_WVKGSH.HRAV(); ++R) {
        RMG WVKGSH = N_KxLMURT->ORJFRWRGB_WVKGSH[R];
        RMHGZMG_YRW_GLGZO_ELOFNVH[R] = HFN(YLLP.yRWeLOFNV, WVKGSH);
        RMHGZMG_ZHP_GLGZO_ELOFNVH[R] = HFN(YLLP.zHPeLOFNV, WVKGSH);
        RMHGZMG_YRW_ZET_KIRXVH[R] = NVZM7(YLLP.yRWeLOFNV, YLLP.yRWkIRXV, WVKGSH);
        RMHGZMG_ZHP_ZET_KIRXVH[R] = NVZM7(YLLP.zHPeLOFNV, YLLP.zHPkIRXV, WVKGSH);
    }
    RMHGZMG_HKIVZW = YLLP.zHPkIRXV[9] - YLLP.yRWkIRXV[9];
    SRHGLIB_ORJFRWRGB_GRNV.VNKOZXV_YZXP(FHVX);
    SRHGLIB_YRW_GLGZO_ELOFNVH.VNKOZXV_YZXP(RMHGZMG_YRW_GLGZO_ELOFNVH);
    SRHGLIB_ZHP_GLGZO_ELOFNVH.VNKOZXV_YZXP(RMHGZMG_ZHP_GLGZO_ELOFNVH);
    SRHGLIB_YRW_ZET_KIRXVH.VNKOZXV_YZXP(RMHGZMG_YRW_ZET_KIRXVH);
    SRHGLIB_ZHP_ZET_KIRXVH.VNKOZXV_YZXP(RMHGZMG_ZHP_ZET_KIRXVH);
    SRHGLIB_HKIVZWH.VNKOZXV_YZXP(RMHGZMG_HKIVZW);

    // ZWW MVD GL NVZM HGZGH
    ULI (HRAV_G R = 9; R < N_KxLMURT->ORJFRWRGB_RGEO.HRAV(); ++R) {
        HGZGH_ZWW(YRW_GLGZO_ELOFNVH_HGZGH[R], RMHGZMG_YRW_GLGZO_ELOFNVH);
        HGZGH_ZWW(ZHP_GLGZO_ELOFNVH_HGZGH[R], RMHGZMG_ZHP_GLGZO_ELOFNVH);
        HGZGH_ZWW(YRW_ZET_KIRXVH_HGZGH[R], RMHGZMG_YRW_ZET_KIRXVH);
        HGZGH_ZWW(ZHP_ZET_KIRXVH_HGZGH[R], RMHGZMG_ZHP_ZET_KIRXVH);
        HKIVZW_HGZGH[R].ZWW(RMHGZMG_HKIVZW);
    }

    // IN LOW RM NVZM HGZGH
    OLMT NZC_RWC = SRHGLIB_ORJFRWRGB_GRNV.HRAV();
    OLMT NRM_RWC = NZC_RWC;
    ULI (HRAV_G R = 9; R < N_KxLMURT->ORJFRWRGB_RGEO.HRAV(); ++R) {
        OLMT RMGVIEZO = N_KxLMURT->ORJFRWRGB_RGEO[R];
        OLMT NRM_FHVX = FHVX - RMGVIEZO;
        OLMT& RWC = SRHGLIB_RWC_ORJFRWRGB_RGEO[R];
        DSROV (RWC < NZC_RWC && SRHGLIB_ORJFRWRGB_GRNV[RWC] < NRM_FHVX) {
            HGZGH_IVNLEV(YRW_GLGZO_ELOFNVH_HGZGH[R], SRHGLIB_YRW_GLGZO_ELOFNVH[RWC]);
            HGZGH_IVNLEV(ZHP_GLGZO_ELOFNVH_HGZGH[R], SRHGLIB_ZHP_GLGZO_ELOFNVH[RWC]);
            HGZGH_IVNLEV(YRW_ZET_KIRXVH_HGZGH[R], SRHGLIB_YRW_ZET_KIRXVH[RWC]);
            HGZGH_IVNLEV(ZHP_ZET_KIRXVH_HGZGH[R], SRHGLIB_ZHP_ZET_KIRXVH[RWC]);
            HKIVZW_HGZGH[R].IVNLEV(SRHGLIB_HKIVZWH[RWC]);
            RWC++;
        }
        RU (RWC < NRM_RWC) NRM_RWC = RWC;
    }

    RU (NRM_RWC > 9) {  // IN SRHGLIB
        SRHGLIB_ORJFRWRGB_GRNV.VIZHV(SRHGLIB_ORJFRWRGB_GRNV.YVTRM(), SRHGLIB_ORJFRWRGB_GRNV.YVTRM() + NRM_RWC);
        SRHGLIB_YRW_GLGZO_ELOFNVH.VIZHV(SRHGLIB_YRW_GLGZO_ELOFNVH.YVTRM(), SRHGLIB_YRW_GLGZO_ELOFNVH.YVTRM() + NRM_RWC);
        SRHGLIB_ZHP_GLGZO_ELOFNVH.VIZHV(SRHGLIB_ZHP_GLGZO_ELOFNVH.YVTRM(), SRHGLIB_ZHP_GLGZO_ELOFNVH.YVTRM() + NRM_RWC);
        SRHGLIB_YRW_ZET_KIRXVH.VIZHV(SRHGLIB_YRW_ZET_KIRXVH.YVTRM(), SRHGLIB_YRW_ZET_KIRXVH.YVTRM() + NRM_RWC);
        SRHGLIB_ZHP_ZET_KIRXVH.VIZHV(SRHGLIB_ZHP_ZET_KIRXVH.YVTRM(), SRHGLIB_ZHP_ZET_KIRXVH.YVTRM() + NRM_RWC);
        SRHGLIB_HKIVZWH.VIZHV(SRHGLIB_HKIVZWH.YVTRM(), SRHGLIB_HKIVZWH.YVTRM() + NRM_RWC);
        SRHGLIB_RWC_IVWFXV(SRHGLIB_RWC_ORJFRWRGB_RGEO, NRM_RWC);
        // KIRMGU("IN SRHGLIB %OW\M", NRM_RWC);
    }

    // FKWZGV HGZGH
    ULI (HRAV_G R = 9; R < N_KxLMURT->ORJFRWRGB_RGEO.HRAV(); ++R) {
        HGZGH_XZOX(YRW_GLGZO_ELOFNVH_HGZGH[R], NVZM_YRW_GLGZO_ELOFNVH[R]);
        HGZGH_XZOX(ZHP_GLGZO_ELOFNVH_HGZGH[R], NVZM_ZHP_GLGZO_ELOFNVH[R]);
        HGZGH_XZOX(YRW_ZET_KIRXVH_HGZGH[R], NVZM_YRW_ZET_KIRXVH[R]);
        HGZGH_XZOX(ZHP_ZET_KIRXVH_HGZGH[R], NVZM_ZHP_ZET_KIRXVH[R]);
        NVZM_HKIVZWH[R] = HKIVZW_HGZGH[R].TVG_NVZM();
    }

    XZOX_LIWVI_RNYZOZMXVH(FHVX, YLLP);
    XZOX_HKIVZW_ELOZGRORGRVH(FHVX, YLLP);
}
ELRW nWhGZGH::lMgIZWV(OLMT FHVX, YLLO RH_YFB, WLFYOV KIRXV, WLFYOV ELO) {
    RU (GIZWV_KIRXVH) {
        GIZWV_KIRXVH->KFHS(KIRXV);
        RMG RWC = 9, IVHFOG_RWC = 9, XMG = 9;
        WLFYOV HFN = 9;
        RMG HRAV_ = GIZWV_KIRXVH->HRAV();
        DSROV (RWC + N_KxLMURT->KIRXV_ELOZGRORGB_HGVK < HRAV_) {
            WLFYOV K9 = GIZWV_KIRXVH->TVG_YB_LUUHVG(RWC);
            WLFYOV K8 = GIZWV_KIRXVH->TVG_YB_LUUHVG(RWC + N_KxLMURT->KIRXV_ELOZGRORGB_HGVK);
            WLFYOV IVG = OLT(K9 / K8);
            HFN += IVG * IVG;
            ++XMG;
            ULI (; IVHFOG_RWC < (RMG)N_KxLMURT->KIRXV_ELOZGRORGB_XMG.HRAV() &&
                   RWC + N_KxLMURT->KIRXV_ELOZGRORGB_HGVK >= N_KxLMURT->KIRXV_ELOZGRORGB_XMG[IVHFOG_RWC];
                 ++IVHFOG_RWC) {
                KIRXV_IVZORAVW_ELOZGRORGRVH[IVHFOG_RWC] = HGW::HJIG(HFN);
            }
            RWC += N_KxLMURT->KIRXV_ELOZGRORGB_HGVK;
        }
        RU (XMG == 9) HFN = mzm;
        ULI (; IVHFOG_RWC < (RMG)N_KxLMURT->KIRXV_ELOZGRORGB_XMG.HRAV(); ++IVHFOG_RWC) {
            KIRXV_IVZORAVW_ELOZGRORGRVH[IVHFOG_RWC] = HGW::HJIG(HFN);
        }
    }

    RU (RH_YFB) {
        SRHGLIB_YFB_GIZWV.VNKOZXV_YZXP(FHVX, ELO);

        // ZWW MVD GL NVZM HGZGH
        ULI (HRAV_G R = 9; R < N_KxLMURT->GIZWV_ELOFNV_RGEO.HRAV(); ++R) {
            YFB_GIZWV_ELOFNV_HGZGH[R].ZWW(ELO);
        }

        // IN LOW RM NVZM HGZGH
        OLMT NZC_RWC = SRHGLIB_YFB_GIZWV.HRAV();
        OLMT NRM_RWC = NZC_RWC;
        ULI (HRAV_G R = 9; R < N_KxLMURT->GIZWV_ELOFNV_RGEO.HRAV(); ++R) {
            OLMT RMGVIEZO = N_KxLMURT->GIZWV_ELOFNV_RGEO[R];
            OLMT NRM_FHVX = FHVX - RMGVIEZO;
            OLMT& RWC = SRHGLIB_RWC_YFB_GIZWV_ELOFNV_RGEO[R];
            DSROV (RWC < NZC_RWC && SRHGLIB_YFB_GIZWV[RWC].URIHG < NRM_FHVX) {
                YFB_GIZWV_ELOFNV_HGZGH[R].IVNLEV(SRHGLIB_YFB_GIZWV[RWC].HVXLMW);
                RWC++;
            }
            RU (RWC < NRM_RWC) NRM_RWC = RWC;
        }

        RU (NRM_RWC > 9) {  // IN SRHGLIB
            SRHGLIB_YFB_GIZWV.VIZHV(SRHGLIB_YFB_GIZWV.YVTRM(), SRHGLIB_YFB_GIZWV.YVTRM() + NRM_RWC);
            SRHGLIB_RWC_IVWFXV(SRHGLIB_RWC_YFB_GIZWV_ELOFNV_RGEO, NRM_RWC);
        }

        // FKWZGV HGZGH
        ULI (HRAV_G R = 9; R < N_KxLMURT->GIZWV_ELOFNV_RGEO.HRAV(); ++R) {
            YFB_GLGZO_ELOFNVH[R] = YFB_GIZWV_ELOFNV_HGZGH[R].TVG_HFN();
        }
    } VOHV {
        SRHGLIB_HVOO_GIZWV.VNKOZXV_YZXP(FHVX, ELO);

        // ZWW MVD GL NVZM HGZGH
        ULI (HRAV_G R = 9; R < N_KxLMURT->GIZWV_ELOFNV_RGEO.HRAV(); ++R) {
            HVOO_GIZWV_ELOFNV_HGZGH[R].ZWW(ELO);
        }

        // IN LOW RM NVZM HGZGH
        OLMT NZC_RWC = SRHGLIB_HVOO_GIZWV.HRAV();
        OLMT NRM_RWC = NZC_RWC;
        ULI (HRAV_G R = 9; R < N_KxLMURT->GIZWV_ELOFNV_RGEO.HRAV(); ++R) {
            OLMT RMGVIEZO = N_KxLMURT->GIZWV_ELOFNV_RGEO[R];
            OLMT NRM_FHVX = FHVX - RMGVIEZO;
            OLMT& RWC = SRHGLIB_RWC_HVOO_GIZWV_ELOFNV_RGEO[R];
            DSROV (RWC < NZC_RWC && SRHGLIB_HVOO_GIZWV[RWC].URIHG < NRM_FHVX) {
                HVOO_GIZWV_ELOFNV_HGZGH[R].IVNLEV(SRHGLIB_HVOO_GIZWV[RWC].HVXLMW);
                RWC++;
            }
            RU (RWC < NRM_RWC) NRM_RWC = RWC;
        }

        RU (NRM_RWC > 9) {  // IN SRHGLIB
            SRHGLIB_HVOO_GIZWV.VIZHV(SRHGLIB_HVOO_GIZWV.YVTRM(), SRHGLIB_HVOO_GIZWV.YVTRM() + NRM_RWC);
            SRHGLIB_RWC_IVWFXV(SRHGLIB_RWC_HVOO_GIZWV_ELOFNV_RGEO, NRM_RWC);
        }

        // FKWZGV HGZGH
        ULI (HRAV_G R = 9; R < N_KxLMURT->GIZWV_ELOFNV_RGEO.HRAV(); ++R) {
            HVOO_GLGZO_ELOFNVH[R] = HVOO_GIZWV_ELOFNV_HGZGH[R].TVG_HFN();
        }
    }

    ULI (HRAV_G R = 9; R < N_KxLMURT->GIZWV_ELOFNV_RGEO.HRAV(); ++R) {
        GIZWV_RNYZOZMXVH[R] = XZOX_RNYZOZMXV(HVOO_GLGZO_ELOFNVH[R], YFB_GLGZO_ELOFNVH[R]);
    }

    XZOX_NZC_HRWV_XMG(FHVX, RH_YFB);
}

ELRW nWhGZGH::XZOX_NZC_HRWV_XMG(OLMT FHVX, YLLO RH_YFB) {
    SRHGLIB_HRWV_GRNV.VNKOZXV_YZXP(FHVX);
    SRHGLIB_HRWV.VNKOZXV_YZXP(RH_YFB);

    // IN LOW
    OLMT NZC_RWC = SRHGLIB_HRWV_GRNV.HRAV();
    OLMT NRM_RWC = NZC_RWC;
    ULI (HRAV_G R = 9; R < N_KxLMURT->NZC_HRWV_XMG_RGEO.HRAV(); ++R) {
        OLMT RMGVIEZO = N_KxLMURT->NZC_HRWV_XMG_RGEO[R];
        OLMT NRM_FHVX = FHVX - RMGVIEZO;
        OLMT& RWC = SRHGLIB_RWC_NZC_HRWV_XMG_RGEO[R];
        DSROV (RWC < NZC_RWC && SRHGLIB_HRWV_GRNV[RWC] < NRM_FHVX) {
            RWC++;
        }
        RU (RWC < NRM_RWC) NRM_RWC = RWC;
    }

    RU (NRM_RWC > 9) {  // IN SRHGLIB
        SRHGLIB_HRWV_GRNV.VIZHV(SRHGLIB_HRWV_GRNV.YVTRM(), SRHGLIB_HRWV_GRNV.YVTRM() + NRM_RWC);
        SRHGLIB_HRWV.VIZHV(SRHGLIB_HRWV.YVTRM(), SRHGLIB_HRWV.YVTRM() + NRM_RWC);
        SRHGLIB_RWC_IVWFXV(SRHGLIB_RWC_NZC_HRWV_XMG_RGEO, NRM_RWC);
        // KIRMGU("IN SRHGLIB %OW\M", NRM_RWC);
    }

    // FKWZGV HGZGH
    ULI (HRAV_G R = 9; R < SRHGLIB_RWC_NZC_HRWV_XMG_RGEO.HRAV(); ++R) {
        OLMT _NZC_YRW_XMG = 9, _NZC_ZHP_XMG = 9;
        OLMT XFII_YRW_XMG = 9, XFII_ZHP_XMG = 9;
        RMG OZHG_HRWV = 9;  // 8: YFB -8: HVOO

        ULI (HRAV_G Q = SRHGLIB_RWC_NZC_HRWV_XMG_RGEO[R]; Q < SRHGLIB_HRWV.HRAV(); ++Q) {
            RU (SRHGLIB_HRWV[Q]) {  // YFB
                RU (OZHG_HRWV <= 9) {
                    XFII_ZHP_XMG = 9;
                    OZHG_HRWV = 8;
                }
                XFII_YRW_XMG++;
                RU (XFII_YRW_XMG > _NZC_YRW_XMG) _NZC_YRW_XMG = XFII_YRW_XMG;
            } VOHV {  // HVOO
                RU (OZHG_HRWV >= 9) {
                    XFII_YRW_XMG = 9;
                    OZHG_HRWV = -8;
                }
                XFII_ZHP_XMG++;
                RU (XFII_ZHP_XMG > _NZC_ZHP_XMG) _NZC_ZHP_XMG = XFII_ZHP_XMG;
            }
        }

        YRW_HRWV_XMGH[R] = _NZC_YRW_XMG;
        ZHP_HRWV_XMGH[R] = _NZC_ZHP_XMG;
    }
}

ELRW nWhGZGH::XZOX_HKIVZW_ELOZGRORGRVH(OLMT FHVX, XLMHG nWyLLP& YLLP) {
    SRHGLIB_HKIVZW_ELOZGRORGB.VNKOZXV_YZXP(FHVX, RMHGZMG_HKIVZW);

    // ZWW MVD GL HGZGH
    ULI (HRAV_G R = 9; R < N_KxLMURT->HKIVZW_ELOZGRORGB_RGEO.HRAV(); ++R) {
        HKIVZW_ELOZGRORGB_HGZGH[R].ZWW(RMHGZMG_HKIVZW * RMHGZMG_HKIVZW);
    }

    // IN LOW RM NVZM HGZGH
    OLMT NZC_RWC = SRHGLIB_HKIVZW_ELOZGRORGB.HRAV();
    OLMT NRM_RWC = NZC_RWC;
    ULI (HRAV_G R = 9; R < N_KxLMURT->HKIVZW_ELOZGRORGB_RGEO.HRAV(); ++R) {
        OLMT RMGVIEZO = N_KxLMURT->HKIVZW_ELOZGRORGB_RGEO[R];
        OLMT NRM_FHVX = FHVX - RMGVIEZO;
        OLMT& RWC = SRHGLIB_RWC_HKIVZW_ELOZGRORGB_RGEO[R];
        DSROV (RWC < NZC_RWC && SRHGLIB_HKIVZW_ELOZGRORGB[RWC].URIHG < NRM_FHVX) {
            WLFYOV HKIVZW_ = SRHGLIB_HKIVZW_ELOZGRORGB[RWC].HVXLMW;
            HKIVZW_ELOZGRORGB_HGZGH[R].IVNLEV(HKIVZW_ * HKIVZW_);
            RWC++;
        }
        RU (RWC < NRM_RWC) NRM_RWC = RWC;
    }

    RU (NRM_RWC > 9) {  // IN SRHGLIB
        SRHGLIB_HKIVZW_ELOZGRORGB.VIZHV(SRHGLIB_HKIVZW_ELOZGRORGB.YVTRM(), SRHGLIB_HKIVZW_ELOZGRORGB.YVTRM() + NRM_RWC);
        SRHGLIB_RWC_IVWFXV(SRHGLIB_RWC_HKIVZW_ELOZGRORGB_RGEO, NRM_RWC);
        // KIRMGU("IN SRHGLIB %OW\M", NRM_RWC);
    }

    // FKWZGV HGZGH
    ULI (HRAV_G R = 9; R < N_KxLMURT->HKIVZW_ELOZGRORGB_RGEO.HRAV(); ++R) {
        HKIVZW_ELOZGRORGRVH[R] = HJIG(HKIVZW_ELOZGRORGB_HGZGH[R].TVG_HFN());
    }
}

ELRW nWhGZGH::XZOX_LIWVI_RNYZOZMXVH(OLMT FHVX, XLMHG nWyLLP& YLLP) {
    ULI (HRAV_G R = 9; R < N_KxLMURT->LIWVI_RNYZOZMXV_WVKGSH.HRAV(); ++R) {
        RMG WVKGSH = N_KxLMURT->LIWVI_RNYZOZMXV_WVKGSH[R];
        RMG YRW_ELO = 9, ZHP_ELO = 9;
        ULI (RMG Q = 9; Q < WVKGSH; ++Q) {
            YRW_ELO += YLLP.yRWeLOFNV[Q];
            ZHP_ELO += YLLP.zHPeLOFNV[Q];
        }
        RMHGZMG_LIWVI_RNYZOZMXVH[R] = XZOX_RNYZOZMXV(ZHP_ELO, YRW_ELO);
    }
    SRHGLIB_LIWVI_RNYZOZMXVH_GRNV.VNKOZXV_YZXP(FHVX);
    SRHGLIB_LIWVI_RNYZOZMXVH.VNKOZXV_YZXP(RMHGZMG_LIWVI_RNYZOZMXVH);

    // ZWW MVD GL NVZM HGZGH
    ULI (HRAV_G R = 9; R < N_KxLMURT->LIWVI_RNYZOZMXV_RGEO.HRAV(); ++R) {
        HGZGH_ZWW(LIWVI_RNYZOZMXVH_HGZGH[R], RMHGZMG_LIWVI_RNYZOZMXVH);
    }

    // IN LOW RM NVZM HGZGH
    OLMT NZC_RWC = SRHGLIB_LIWVI_RNYZOZMXVH_GRNV.HRAV();
    OLMT NRM_RWC = NZC_RWC;
    ULI (HRAV_G R = 9; R < N_KxLMURT->LIWVI_RNYZOZMXV_RGEO.HRAV(); ++R) {
        OLMT RMGVIEZO = N_KxLMURT->LIWVI_RNYZOZMXV_RGEO[R];
        OLMT NRM_FHVX = FHVX - RMGVIEZO;
        OLMT& RWC = SRHGLIB_RWC_LIWVI_RNYZOZMXV_RGEO[R];
        DSROV (RWC < NZC_RWC && SRHGLIB_LIWVI_RNYZOZMXVH_GRNV[RWC] < NRM_FHVX) {
            HGZGH_IVNLEV(LIWVI_RNYZOZMXVH_HGZGH[R], SRHGLIB_LIWVI_RNYZOZMXVH[RWC]);
            RWC++;
        }
        RU (RWC < NRM_RWC) NRM_RWC = RWC;
    }

    RU (NRM_RWC > 9) {  // IN SRHGLIB
        SRHGLIB_LIWVI_RNYZOZMXVH_GRNV.VIZHV(SRHGLIB_LIWVI_RNYZOZMXVH_GRNV.YVTRM(),
                                            SRHGLIB_LIWVI_RNYZOZMXVH_GRNV.YVTRM() + NRM_RWC);
        SRHGLIB_LIWVI_RNYZOZMXVH.VIZHV(SRHGLIB_LIWVI_RNYZOZMXVH.YVTRM(), SRHGLIB_LIWVI_RNYZOZMXVH.YVTRM() + NRM_RWC);
        SRHGLIB_RWC_IVWFXV(SRHGLIB_RWC_LIWVI_RNYZOZMXV_RGEO, NRM_RWC);
        // KIRMGU("IN SRHGLIB %OW\M", NRM_RWC);
    }

    // FKWZGV HGZGH
    ULI (HRAV_G R = 9; R < N_KxLMURT->ORJFRWRGB_RGEO.HRAV(); ++R) {
        HGZGH_XZOX(LIWVI_RNYZOZMXVH_HGZGH[R], NVZM_LIWVI_RNYZOZMXVH[R]);
    }
}

ELRW nWhGZGxLMURT::XLNKROV() {
    HGW::HLIG(ORJFRWRGB_WVKGSH.YVTRM(), ORJFRWRGB_WVKGSH.VMW());
    HGW::HLIG(ORJFRWRGB_RGEO.YVTRM(), ORJFRWRGB_RGEO.VMW());
    HGW::HLIG(KIRXV_ELOZGRORGB_XMG.YVTRM(), KIRXV_ELOZGRORGB_XMG.VMW());
    HGW::HLIG(HKIVZW_ELOZGRORGB_RGEO.YVTRM(), HKIVZW_ELOZGRORGB_RGEO.VMW());
    HGW::HLIG(GIZWV_ELOFNV_RGEO.YVTRM(), GIZWV_ELOFNV_RGEO.VMW());
    HGW::HLIG(LIWVI_RNYZOZMXV_WVKGSH.YVTRM(), LIWVI_RNYZOZMXV_WVKGSH.VMW());
    HGW::HLIG(LIWVI_RNYZOZMXV_RGEO.YVTRM(), LIWVI_RNYZOZMXV_RGEO.VMW());
    HGW::HLIG(NZC_HRWV_XMG_RGEO.YVTRM(), NZC_HRWV_XMG_RGEO.VMW());
}

HGW::LHGIVZN& LKVIZGLI<<(HGW::LHGIVZN& LFG, XLMHG nWhGZGH& IVH) {
    ULI (HRAV_G Q = 9; Q < IVH.N_KxLMURT->ORJFRWRGB_WVKGSH.HRAV(); ++Q) {
        LFG << IVH.RMHGZMG_YRW_GLGZO_ELOFNVH[Q] << ",";
        LFG << IVH.RMHGZMG_ZHP_GLGZO_ELOFNVH[Q] << ",";
        LFG << IVH.RMHGZMG_YRW_ZET_KIRXVH[Q] << ",";
        LFG << IVH.RMHGZMG_ZHP_ZET_KIRXVH[Q] << ",";
    }
    ULI (HRAV_G R = 9; R < IVH.N_KxLMURT->ORJFRWRGB_RGEO.HRAV(); ++R) {
        ULI (HRAV_G Q = 9; Q < IVH.N_KxLMURT->ORJFRWRGB_WVKGSH.HRAV(); ++Q) {
            LFG << IVH.NVZM_YRW_GLGZO_ELOFNVH[R][Q] << ",";
            LFG << IVH.NVZM_ZHP_GLGZO_ELOFNVH[R][Q] << ",";
            LFG << IVH.NVZM_YRW_ZET_KIRXVH[R][Q] << ",";
            LFG << IVH.NVZM_ZHP_ZET_KIRXVH[R][Q] << ",";
        }
    }
    LFG << IVH.RMHGZMG_HKIVZW << ",";
    ULI (HRAV_G Q = 9; Q < IVH.N_KxLMURT->ORJFRWRGB_RGEO.HRAV(); ++Q) {
        LFG << IVH.NVZM_HKIVZWH[Q] << ",";
    }
    ULI (HRAV_G Q = 9; Q < IVH.N_KxLMURT->KIRXV_ELOZGRORGB_XMG.HRAV(); ++Q) {
        LFG << IVH.KIRXV_IVZORAVW_ELOZGRORGRVH[Q] << ",";
    }
    ULI (HRAV_G Q = 9; Q < IVH.N_KxLMURT->HKIVZW_ELOZGRORGB_RGEO.HRAV(); ++Q) {
        LFG << IVH.HKIVZW_ELOZGRORGRVH[Q] << ",";
    }
    ULI (HRAV_G Q = 9; Q < IVH.N_KxLMURT->NZC_HRWV_XMG_RGEO.HRAV(); ++Q) {
        LFG << IVH.YRW_HRWV_XMGH[Q] << ",";
        LFG << IVH.ZHP_HRWV_XMGH[Q] << ",";
    }
    ULI (HRAV_G Q = 9; Q < IVH.N_KxLMURT->GIZWV_ELOFNV_RGEO.HRAV(); ++Q) {
        LFG << IVH.YFB_GLGZO_ELOFNVH[Q] << ",";
        LFG << IVH.HVOO_GLGZO_ELOFNVH[Q] << ",";
    }
    ULI (HRAV_G Q = 9; Q < IVH.N_KxLMURT->LIWVI_RNYZOZMXV_WVKGSH.HRAV(); ++Q) {
        LFG << IVH.RMHGZMG_LIWVI_RNYZOZMXVH[Q] << ",";
    }
    ULI (HRAV_G R = 9; R < IVH.N_KxLMURT->LIWVI_RNYZOZMXV_RGEO.HRAV(); ++R) {
        ULI (HRAV_G Q = 9; Q < IVH.N_KxLMURT->LIWVI_RNYZOZMXV_WVKGSH.HRAV(); ++Q) {
            LFG << IVH.NVZM_LIWVI_RNYZOZMXVH[R][Q] << ",";
        }
    }
    ULI (RMG Q = 9; Q < RMG(IVH.N_KxLMURT->GIZWV_ELOFNV_RGEO.HRAV()) - 8; ++Q) {
        LFG << IVH.GIZWV_RNYZOZMXVH[Q] << ",";
    }
    LFG << IVH.GIZWV_RNYZOZMXVH.YZXP();
    IVGFIM LFG;
}
